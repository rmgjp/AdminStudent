<title>{{asignatura}} {{clave}} - Alumnos - Seguimiento de Estudiantes</title>

<div class="container bg-light">
    <div class="row">
        <ul class="nav nav-tabs">
            <li class="nav-item">
                <a class="nav-link bi bi-house-fill" aria-current="page" href="/"></a>
            </li>
            <li class="nav-item">
                <a class="nav-link " aria-current="page" href="/grupo-inicio/{{idgrupo}}">Inicio</a>
            </li>
            <li class="nav-item">
                <a class="nav-link active" href="/grupo/alumnos/{{idgrupo}}">Alumnos</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/grupo/temas/{{idgrupo}}">Temas</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/grupo/actividades/{{idgrupo}}">Actividades</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/grupo/calificaciones/{{idgrupo}}">Calificaciones</a>
            </li>
        </ul>
    </div>
    <div class="row m-2 p-3">
        <h2 class="">Lista de Alumnos</h2>
    </div>
    <div class="row m-2 p-0">
            <div class="col-auto">
                <div class="list-group">
                    {{#each alumnos}}
                        <a type="button" class="list-group-item list-group-item-action" aria-current="true" href="#">
                            <strong>{{dataValues.clave}}</strong> - {{dataValues.apellidos}} {{dataValues.nombre}}
                        </a>
                    {{/each}}
                </div>
            </div>
        <div class="col-auto">

        </div>


    </div>
    <script type="javascript">
        function goBack() {
            window.history.back();
        }
    </script>
</div>