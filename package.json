    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.5.0",
    "bootstrap-table": "^1.18.3",
    "bs-custom-file-input": "^1.3.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "electron-first-run": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
